{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "required": [
    "ServiceName",
    "VpcId",
    "SecurityGroupIds",
    "SubnetIds",
    "VpcEndpointType"
  ],
  "additionalProperties": false,
  "properties": {
    "ServiceName": {
      "type": "string"
    },
    "VpcId": {
      "type": "string"
    },
    "SecurityGroupIds" : {
        "type" : "array",
        "items" : {
             "type": "string",
             "pattern" : "pattern": "^[a-z][a-z0-9\\-]+$"
        }
    },
    "SubnetIds" : {
        "type" : "array",
        "items" : {
             "type": "string",
             "pattern" : "pattern": "^[a-z][a-z0-9\\-]+$"
        }
    },
    "VpcEndpointType" : {
        "type" : "string",
        "enum" : [
            "Interface"
        ]
    }
  }
}